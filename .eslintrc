{
    "parser": "babel-eslint",

    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb",
    "rules": {
        "no-underscore-dangle": ["error", { "allow": ["__REDUX_DEVTOOLS_EXTENSION_COMPOSE__"] }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }     
}